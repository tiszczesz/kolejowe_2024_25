@model List<MyColor>
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>
<body>
    <h1>Lista kolorów</h1>
    <select id="colors">
        @foreach(var c in Model){
            <option value="@c.Key">@c.Value</option>
        }
    </select>
    <script src="~/myscript.js" defer></script>
</body>
</html>
